{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"lib": ["es2017", "dom", "dom.iterable"],
		"module": "esnext",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": true,
		"noEmit": true,
		"noImplicitReturns": false,
		"noImplicitAny": false,
		"noUnusedLocals": false,
		"outDir": "../../../dist",
		"paths": {
			"codestream-common/agent-protocol": [
				"../../../../shared/ui/protocols/agent/agent.protocol.ts"
			],
			"codestream-common/api-protocol": ["../../../../shared/ui/protocols/agent/api.protocol.ts"],
			"@codestream/protocols/webview": ["../../../../shared/ui/ipc/webview.protocol.ts"],
			"@codestream/webview/*": ["../../../../shared/ui/*"],
			"vscode-jsonrpc": ["../../../../shared/ui/vscode-jsonrpc.shim.ts"]
		},
		"rootDir": ".",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "es2017"
	}
}
